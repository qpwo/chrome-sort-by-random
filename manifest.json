{
    "name": "Randomize list",
    "version": "1.2",
    "description": "Randomize the order of items in a given list",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {},
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": "Ctrl+Shift+0",
            "description": "Choose element to randomize"
        }
    }
}